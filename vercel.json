{
    "version":2,
    "builds":[
        {
            "src":"./harvester.py",
            "use":"@vercel/python"
        }
    ],

    "routes":[
        {
            "src":"/(.*)",
            "dest":"/provider.py",
            "methods":["GET", "POST", "PUT"],
            "headers":{
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}