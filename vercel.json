{
    "version":2,
    "builds":[
        {
            "src":"./provider.py",
            "use":"@vercel/python"
        },
        {
            "src":"./harvester.py",
            "use":"@vercel/python"
        }
    ],

    "routes":[
        {
            "src":"/(.*)",
            "dest":"/provider.py",
            "methods":["GET", "POST", "PUT"],
            "headers":{
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "POST"
            }

        },

        {
            "src":"/OpenVisualDatabase",
            "dest":"/harvester.py",
            "methods":["GET", "POST", "PUT"],
            "headers":{
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "POST"
            }

        },

        {
            "src":"/Automations",
            "dest":"/harvester.py",
            "methods":["GET", "POST", "PUT"],
            "headers":{
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "POST"
            }

        }
        
    ]
      
}