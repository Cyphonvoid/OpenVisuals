{
    "version":2,
    "builds":[
        {
            "src":"./harvester.py",
            "use":"@vercel/python"
        },
        {
            "src":"./provider.py",
            "use":"@vercel/python"
         }
    ],

    "routes":[
        {
            "src":"/(.*)",
            "dest":"/harvester.py",
            "methods":["GET", "POST", "PUT"],
            "headers":{
                "Access-Control-Allow-Origin": "*"
            }
        },
        {
            "src":"/(.*)",
            "dest":"/provider.py",
            "methods":["GET", "POST", "PUT"],
            "headers":{
                "Access-Control-Allow-Origin": "*"
            }
        }

    
    ]
}